<?xml version='1.0' encoding='utf-8'?>
<widget id="com.peane1.rtuinfo" version="0.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>RTU info</name>
    <description>
        RTU info for Android.
    </description>
    <author email="notzipo@msn.com" href="http://not.zz.mu">
        notzipo
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="icon.png" />
    <icon src="res/android/launcher-icon-0-75x.png"   gap:platform="android"    gap:qualifier="ldpi" />
    <icon src="res/android/launcher-icon-1x.png"   gap:platform="android"    gap:qualifier="mdpi" />
    <icon src="res/android/launcher-icon-2x.png"   gap:platform="android"    gap:qualifier="hdpi" />
    <icon src="res/android/launcher-icon-256.png"  gap:platform="android"    gap:qualifier="xhdpi" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
